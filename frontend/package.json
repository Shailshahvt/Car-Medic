  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm -rf ../backend/public && cp -r build ../backend/public",
    "dev": "nodemon --watch src --ext js,jsx,css,html --ignore build/ --exec \"npm run build\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*",
      "node_modules/*"
    ],
    "delay": "2500"
  }
}
